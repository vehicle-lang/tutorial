  \definecolor{beamer@headercolor}{HTML}{6e4863}
  \definecolor{beamer@normaltextcolor}{HTML}{444444}
  \definecolor{beamer@progresscolor}{HTML}{a33c86}
  \definecolor{aisecyellow}{RGB}{255, 196, 35}
  \definecolor{aisecred}{RGB}{204, 72, 71}
  \definecolor{aisecpurple}{RGB}{92,95,173}

  \colorlet{beamer@barcolor}{beamer@headercolor!16!white}
